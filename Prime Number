
import matplotlib.pyplot as plt 
import numpy as np 
import array as arr 
import pandas as pd 

#WAP to check weather the no is Prime number or not
a = int(input("Enter a Number : "))

if a > 1 :
	for x in range(2,a):
		if(a%x) == 0:
			print("Not Prime")
			break
		else:
			print("Prime")
			break

else:
	print("Not Prime")
